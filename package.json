    "babel-loader": "^8.0.6",
    "babel-plugin-react-require": "^3.1.1",
    "chalk": "^3.0.0",
    "cross-env": "^6.0.3",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "i18next-scanner": "^2.10.3",
    "jest": "^24.9.0",
    "node-sass": "^4.13.0",
