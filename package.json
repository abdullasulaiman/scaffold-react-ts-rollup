  "main": "public/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development rollup -c -w",
    "build": "cross-env NODE_ENV=production rollup -c",
    "prebuild": "npm-run-all --parallel fix:eslint fix:prettier build:i18",
    "serve": "serve dist/public",
    "dev:watch": "npm run build:i18 && npm-run-all --parallel dev serve dev:fix-lint",
    "test": "jest --coverage",
    "storybook": "start-storybook",
    "dev:fix-lint": "onchange 'src/**/*.*' -- npm-run-all --parallel fix:eslint fix:prettier build:i18",
    "fix:eslint": "eslint --fix \"src/*.*\"",
    "fix:prettier": "prettier --write \"src/*.*\"",
    "build:i18": "i18next-scanner --config i18next-scanner.config.js"
  },
  "dependencies": {
    "@thoughtspot/array-util": "*",
    "@thoughtspot/js-util":"*",
    "@types/react": "^16.9.11",
